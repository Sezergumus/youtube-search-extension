{
    "name": "Easy YouTube Search",
    "description": "Type 'y' plus a search term into the Omnibox to search in YouTube easily.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "omnibox": { "keyword" : "y" },
    "action": {
      "default_icon": {
        "16": "youtubeLogo.png",
        "32": "youtubeLogo.png"
        }
    },
    "icons": {
      "128": "youtubeLogo.png"
    }
}